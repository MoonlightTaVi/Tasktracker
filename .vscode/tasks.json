                "${workspaceFolder}\\tools\\starts-with.cpp",
                "${workspaceFolder}\\tools\\parse-int.cpp",
                "${workspaceFolder}\\tools\\datetime.cpp",
                "${workspaceFolder}\\core\\tasktracker.cpp",
